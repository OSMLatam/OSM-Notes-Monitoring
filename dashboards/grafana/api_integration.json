  "panels": [
    {
      "id": 1,
      "title": "API Success Rate (%)",
      "type": "gauge",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "targets": [{
        "datasource": {
          "type": "postgres",
          "uid": "PostgreSQL"
        },
        "editorMode": "code",
        "format": "table",
        "rawQuery": true,
        "rawSql": "SELECT metric_value as value FROM metrics WHERE component = 'ingestion' AND metric_name = 'api_success_rate_percent' ORDER BY timestamp DESC LIMIT 1",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 95, "color": "yellow"},
              {"value": 100, "color": "green"}
            ]
          }
        }
      }
    },
