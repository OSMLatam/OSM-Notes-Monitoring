      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "gray"},
              {"value": 0, "color": "gray"},
              {"value": 1, "color": "green"}
            ]
          },
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {"text": "No data", "color": "gray"},
                "null": {"text": "No data", "color": "gray"}
              }
            },
            {
              "type": "special",
              "options": {
                "match": "null",
                "result": {"text": "No data", "color": "gray"}
              }
            }
          ]
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": true,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "auto"
      }
