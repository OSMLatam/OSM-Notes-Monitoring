        "editorMode": "code",
        "format": "table",
        "rawQuery": true,
        "rawSql": "SELECT COALESCE((SELECT metric_value FROM metrics WHERE component = 'ingestion' AND metric_name = 'log_cycle_number' ORDER BY timestamp DESC LIMIT 1), 0) as value",
        "refId": "A"
