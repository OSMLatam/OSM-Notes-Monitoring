  "panels": [
    {
      "id": 1,
      "title": "Cycle Total Duration (seconds)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [{
        "datasource": {
          "type": "postgres",
          "uid": "PostgreSQL"
        },
        "editorMode": "code",
        "format": "time_series",
        "rawQuery": true,
        "rawSql": "SELECT timestamp AS time, metric_value AS value FROM metrics WHERE component = 'ingestion' AND metric_name = 'log_cycle_total_duration_seconds' AND $__timeFilter(timestamp) ORDER BY timestamp",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 1,
          "min": 0
        },
        "overrides": [
          {
            "matcher": {"id": "byFrameRefID", "options": "A"},
            "properties": [
              {"id": "displayName", "value": "Total Duration (s)"},
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}
            ]
          }
        ]
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "bottom"},
        "tooltip": {"mode": "multi"}
      }
    },
