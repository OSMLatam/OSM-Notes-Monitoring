        "datasource": {
          "type": "postgres",
          "uid": "PostgreSQL"
        },
        "editorMode": "code",
        "format": "table",
        "rawQuery": true,
        "rawSql": "SELECT COALESCE((SELECT metric_value FROM metrics WHERE component = 'ingestion' AND metric_name = 'log_cycle_success_rate_percent' ORDER BY timestamp DESC LIMIT 1), 100) as value",
        "refId": "A"
