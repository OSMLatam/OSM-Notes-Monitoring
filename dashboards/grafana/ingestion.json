    {
      "id": 15,
      "title": "Notes Check Validator Status",
      "type": "table",
      "gridPos": {"h": 6, "w": 10, "x": 4, "y": 30},
      "targets": [{
        "datasource": {"type": "postgres", "uid": "PostgreSQL"},
        "editorMode": "code",
        "format": "table",
        "rawQuery": true,
        "rawSql": "SELECT 'Validator Status' as check_type, CASE WHEN COALESCE((SELECT metric_value FROM metrics WHERE component = 'ingestion' AND metric_name = 'data_quality_check_status' ORDER BY timestamp DESC LIMIT 1), -1) = 1 THEN 'Passed' WHEN (SELECT metric_value FROM metrics WHERE component = 'ingestion' AND metric_name = 'data_quality_check_status' ORDER BY timestamp DESC LIMIT 1) = 0 THEN 'Failed' WHEN (SELECT metric_value FROM metrics WHERE component = 'ingestion' AND metric_name = 'data_quality_check_status' ORDER BY timestamp DESC LIMIT 1) = 2 THEN 'Running' ELSE 'Unknown' END as status UNION ALL SELECT 'Last Run', COALESCE(TO_CHAR((SELECT MAX(timestamp) FROM metrics WHERE component = 'ingestion' AND metric_name = 'data_quality_check_status'), 'YYYY-MM-DD HH24:MI:SS'), 'Never') UNION ALL SELECT 'Notes Not in DB', COALESCE((SELECT metric_value::text FROM metrics WHERE component = 'ingestion' AND metric_name = 'validator_notes_not_in_db_count' ORDER BY timestamp DESC LIMIT 1), 'N/A') UNION ALL SELECT 'Quality Score', COALESCE((SELECT ROUND(metric_value, 1)::text FROM metrics WHERE component = 'ingestion' AND metric_name = 'data_quality_score' ORDER BY timestamp DESC LIMIT 1), 'N/A') || '%'",
        "refId": "A"
      }],
      "options": {
        "showHeader": true,
        "sortBy": []
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "status"},
            "properties": [
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "Passed": {"text": "Passed", "color": "green"},
                      "Failed": {"text": "Failed", "color": "red"},
                      "Running": {"text": "Running", "color": "yellow"},
                      "Unknown": {"text": "Unknown", "color": "gray"}
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
