  "editable": true,
  "panels": [
    {
      "id": 1,
      "title": "Daemon Status",
      "type": "stat",
      "gridPos": {"h": 4, "w": 3, "x": 0, "y": 0},
      "targets": [{
        "datasource": {"type": "postgres", "uid": "PostgreSQL"},
        "editorMode": "code",
        "format": "table",
        "rawQuery": true,
        "rawSql": "SELECT metric_value::numeric as value FROM metrics WHERE component = 'ingestion' AND metric_name = 'daemon_status' ORDER BY timestamp DESC LIMIT 1",
        "refId": "A"
      }],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "1": {"text": "Active", "color": "green"},
                "0": {"text": "Inactive", "color": "red"}
              }
            },
            {
              "type": "special",
              "options": {
                "match": "null",
                "result": {
                  "text": "No data",
                  "color": "gray"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "gray"},
              {"value": 0, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {"values": true, "calcs": ["lastNotNull"], "fields": ""},
        "textMode": "auto"
      }
    },
