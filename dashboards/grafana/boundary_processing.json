        "format": "table",
        "rawQuery": true,
        "rawSql": "SELECT ROUND((SELECT metric_value FROM metrics WHERE component = 'ingestion' AND metric_name = 'boundary_notes_with_country_count' ORDER BY timestamp DESC LIMIT 1) * 100.0 / NULLIF((SELECT metric_value FROM metrics WHERE component = 'ingestion' AND metric_name = 'boundary_notes_without_country_count' ORDER BY timestamp DESC LIMIT 1) + (SELECT metric_value FROM metrics WHERE component = 'ingestion' AND metric_name = 'boundary_notes_with_country_count' ORDER BY timestamp DESC LIMIT 1), 0), 2) as value",
