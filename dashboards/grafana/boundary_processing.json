        "editorMode": "code",
        "format": "table",
        "rawQuery": true,
        "rawSql": "SELECT metric_value as value FROM metrics WHERE component = 'ingestion' AND metric_name = 'boundary_update_frequency_hours' AND metadata LIKE '%type=countries%' ORDER BY timestamp DESC LIMIT 1",
        "refId": "A"
